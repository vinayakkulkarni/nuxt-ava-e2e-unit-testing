{
  "name": "nuxt-ava-e2e-unit-testing",
  "version": "1.0.0",
  "description": "Nuxt app with Ava -&gt; Unit Testing &amp; E2E Testing",
  "author": "Vinayak Kulkarni",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "TEST=unit nyc ava ./test/specs/*.spec.js --verbose --color",
    "test:e2e": "TEST=e2e nyc ava ./test/e2e/*.js --verbose --color",
    "test": "npm run test:unit & npm run test:e2e"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "cross-env": "^5.2.0",
    "element-ui": "^2.4.11",
    "nuxt": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "ava": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.5",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jsdom-global": "^3.0.2",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^2.0.0"
  }
}
