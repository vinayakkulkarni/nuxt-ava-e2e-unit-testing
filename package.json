{
  "name": "nuxt-ava-e2e-unit-testing",
  "version": "1.0.0",
  "description": "Nuxt demo application with Ava, running E2E and Unit tests",
  "author": "Vinayak Kulkarni",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:eslint": "eslint \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,vue}\"",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,vue}\"",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,vue}\" --ignore-path .gitignore",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "precommit": "npm run lint",
    "test:unit": "cross-env TEST=unit nyc ava --config ava.config.cjs ./test/specs/**/*",
    "test:unit:watch": "cross-env TEST=unit nyc ava --config ava.config.cjs --watch",
    "test:e2e": "cross-env TEST=e2e ava --config ava.config.cjs ./test/e2e/**/*",
    "test": "npm run test:unit & npm run test:e2e",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "cross-env": "^7.0.3",
    "element-ui": "^2.15.6",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/test-utils": "^1.3.0",
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "browser-env": "^3.3.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "4.3.8",
    "jsdom-global": "^3.0.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "sinon": "^13.0.1"
  }
}
