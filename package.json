{
  "name": "nuxt-ava-e2e-unit-testing",
  "version": "1.0.0",
  "description": "Nuxt demo application with Ava, running E2E and Unit tests",
  "author": "Vinayak Kulkarni",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "cross-env TEST=unit nyc ava --config unit-tests.config.js  --color",
    "test:unit:watch": "cross-env TEST=unit nyc ava --config unit-tests.config.js --color --watch",
    "test:e2e": "cross-env TEST=e2e nyc ava --config e2e-tests.config.js --verbose --color",
    "test": "npm run test:unit & npm run test:e2e",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.0",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "cross-env": "^6.0.3",
    "element-ui": "^2.13.0",
    "nuxt": "^2.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-env": "^3.3.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.1",
    "husky": "^3.1.0",
    "jsdom-global": "^3.0.2",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "sinon": "^8.0.1"
  }
}
