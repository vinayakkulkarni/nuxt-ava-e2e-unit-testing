{
  "name": "nuxt-ava-e2e-unit-testing",
  "version": "1.0.0",
  "description": "Nuxt demo application with Ava, running E2E and Unit tests",
  "author": "Vinayak Kulkarni",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "cross-env TEST=unit nyc ava --config ava.config.cjs ./test/specs/**/*",
    "test:unit:watch": "cross-env TEST=unit nyc ava --config ava.config.cjs --watch",
    "test:e2e": "cross-env TEST=e2e ava --config ava.config.cjs ./test/e2e/**/*",
    "test": "npm run test:unit & npm run test:e2e",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.3",
    "@nuxtjs/pwa": "^3.3.2",
    "cross-env": "^7.0.3",
    "element-ui": "^2.14.1",
    "nuxt": "^2.14.9"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/preset-env": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/test-utils": "^1.1.1",
    "ava": "^3.13.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-env": "^3.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.4",
    "jsdom-global": "^3.0.2",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "sinon": "^9.2.1"
  }
}
